{
  "cli": {
    "version": ">= 3.23.0",
    "appVersionSource": "remote"
  },

  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "node": "20.19.4",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EAS_BUILD_WORKINGDIR": "client"
      }
    },

    "production": {
      "distribution": "store",
      "autoIncrement": true,
      "node": "20.19.4",
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "EAS_BUILD_WORKINGDIR": "client"
      }
    }
  }
}
