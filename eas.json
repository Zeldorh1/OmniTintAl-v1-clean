{
  "cli": {
    "version": ">= 3.23.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "node": "20.19.4",
      "env": {
        "NODE_ENV": "development",
        "EAS_SKIP_AUTO_FINGERPRINT": "1",
        "EAS_NO_LOCAL_CONFIG": "1"
      }
    },
    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "android": {
        "buildType": "app-bundle"
      },
      "node": "20.19.4",
      "env": {
        "EAS_PUBLIC_ENV": "production"
      }
    }
  }
}
